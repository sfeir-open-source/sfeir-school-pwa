  },
  "keywords": [
    "PWA, service worker, vanillajs, materials"
  ],
  "scripts": {
    "start": "parallelshell \"npm run start-app $npm_config_step\" \"npm run server\"",
    "start-app": "cross-var webpack-dev-server --env step=$npm_config_step",
    "dev": "webpack-dev-server",
    "server": "node ./common/server/server.js",
    "ngrok": "ngrok http --host-header=\"localhost:8080\" 8080 ",
    "server-old": "json-server assets/mocks/people.json --routes assets/mocks/routes.json",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,html,css,scss,md,js,ts}"
  },
  "bugs": {
    "url": "https://github.com/sfeir-open-source/sfeir-school-pwa/issues"
  },
  "homepage": "https://github.com/sfeir-open-source/sfeir-school-pwa#readme",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "google-auth-library": "^9.0.0",
    "idb": "^5.0.4",
    "jose": "^4.14.4",
    "json-server": "^0.16.1",
    "lit-element": "^2.1.0",
    "normalize.css": "8.0.1",
    "pwa-helpers": "^0.9.1"
  },
  "private": true,
  "devDependencies": {
    "cross-var": "^1.1.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^1.3.1",
    "lighthouse": "^6.1.1",
    "lint-staged": "^8.1.5",
    "lodash": ">=4.17.13",
    "ngrok": "^5.0.0-beta.2",
    "parallelshell": "3.0.1",
    "prettier": "^1.16.4",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
